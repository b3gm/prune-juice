{
  "name": "@b3gm/prune-juice",
  "version": "0.0.2",
  "description": "Implementation of sweep and prune",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build-test": "npx tsc && npx browserify dist/prunejuice-test.js -o dist/prunejuice-test.bundle.js",
    "test": "npx jest",
    "prepublishOnly": "rm -rf dist && npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b3gm/prune-juice.git"
  },
  "keywords": [
    "collision_detection",
    "sweep_and_prune"
  ],
  "author": "b3gm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/b3gm/prune-juice/issues"
  },
  "homepage": "https://github.com/b3gm/prune-juice#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.(ts)$",
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "browserify": "^16.5.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.3"
  }
}
